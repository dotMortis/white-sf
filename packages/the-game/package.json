{
    "version": "0.0.0",
    "type": "module",
    "license": "UNLICENSED",
    "scripts": {
        "build": "rm -rf dist/ && tsc -p ./tsconfig.build.json",
        "build:watch": "pnpm build --watch",
        "typecheck": "tsc -p ./tsconfig.build.json --noEmit",
        "typecheck:watch": "pnpm typecheck --watch",
        "lint": "eslint . --fix",
        "test": "vitest",
        "test:coverage": "pnpm test -- --coverage",
        "test:watch": "pnpm test -- --watch",
        "depcheck": "depcheck",
        "prebuild": "node ../../bin/export-definer.mjs"
    },
    "devDependencies": {
        "@bits_devel/eslint-config-bits": "latest",
        "@bits_devel/tsconfig": "latest",
        "@types/node": "^20",
        "ts-node": "latest",
        "prettier": "^3",
        "typescript": "^5",
        "@vitest/coverage-v8": "^2",
        "vitest": "^2"
    },
    "dependencies": {},
    "files": [
        "./dist",
        "./README.md"
    ],
    "exports": {
        "./card-deck": {
            "types": "./src/card-deck.ts",
            "default": "./dist/card-deck.js"
        },
        "./card-suite": {
            "types": "./src/card-suite.ts",
            "default": "./dist/card-suite.js"
        },
        "./card": {
            "types": "./src/card.ts",
            "default": "./dist/card.js"
        },
        "./standard-cards": {
            "types": "./src/cards/standard-cards.ts",
            "default": "./dist/cards/standard-cards.js"
        },
        "./global-values": {
            "types": "./src/global-values.ts",
            "default": "./dist/global-values.js"
        },
        ".": {
            "types": "./src/index.ts",
            "default": "./dist/index.js"
        },
        "./player": {
            "types": "./src/player.ts",
            "default": "./dist/player.js"
        },
        "./vote-maschine": {
            "types": "./src/vote/vote-maschine.ts",
            "default": "./dist/vote/vote-maschine.js"
        },
        "./vote": {
            "types": "./src/vote/vote.ts",
            "default": "./dist/vote/vote.js"
        },
        "./package.json": "./package.json"
    },
    "name": "@internal/the-game",
    "private": true
}
